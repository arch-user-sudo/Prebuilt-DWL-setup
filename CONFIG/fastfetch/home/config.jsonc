{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "kitty-icat",
        "source": "~/.config/fastfetch/images/hypr.png",
        /*"height": 40,
        /*"width": 20,
        /*"padding": {
        /*    "top": 5,
        /*    "left": 3
        }
    },
    "modules": [
        "break",
        {
            "type": "custom",
            "format": "\u001b[90m┌──────────────────────Hardware──────────────────────┐"
        },
        {
            "type": "host",
            "key": " PC",
            "keyColor": "white"
        },
        {
            "type": "cpu",
            "key": "│ ├",
            "keyColor": "white"
        },
        {
            "type": "gpu",
            "key": "│ ├󰍛",
            "keyColor": "white"
        },
        {
            "type": "memory",
            "key": "│ ├󰍛",
            "keyColor": "white"
        },
        {
            "type": "disk",
            "key": "└ └",
            "keyColor": "white"
        },
        {
            "type": "custom",
            "format": "\u001b[90m└────────────────────────────────────────────────────┘"
        },
        "break",
        {
            "type": "custom",
            "format": "\u001b[90m┌──────────────────────Software──────────────────────┐"
        },
        {
            "type": "os",
            "key": " OS",
            "keyColor": "white"
        },
        {
            "type": "kernel",
            "key": "│ ├",
            "keyColor": "white"
        },
        {
            "type": "bios",
            "key": "│ ├",
            "keyColor": "white"
        },
        {
            "type": "packages",
            "key": "│ ├󰏖",
            "keyColor": "white"
        },
        {
            "type": "shell",
            "key": "└ └",
            "keyColor": "white"
        },
        "break",
        {
            "type": "de",
            "key": " DE",
            "keyColor": "white"
        },
        {
            "type": "lm",
            "key": "│ ├",
            "keyColor": "white"
        },
        {
            "type": "wm",
            "key": "│ ├",
            "keyColor": "white"
        },
        {
            "type": "wmtheme",
            "key": "│ ├󰉼",
            "keyColor": "white"
        },
        {
            "type": "terminal",
            "key": "└ └",
            "keyColor": "white"
        },
        {
            "type": "custom",
            "format": "\u001b[90m└────────────────────────────────────────────────────┘"
        },
        "break",
        {
            "type": "custom",
            "format": "\u001b[90m┌────────────────Uptime / Age / DT───────────────────┐"
        },
        {
            "type": "command",
            "key": "  OS Age ",
            "keyColor": "white",
            "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days"
        },
        {
            "type": "uptime",
            "key": "  Uptime ",
            "keyColor": "white"
        },
        {
            "type": "datetime",
            "key": "  DateTime ",
            "keyColor": "white"
        },
        {
            "type": "custom",
            "format": "\u001b[90m└────────────────────────────────────────────────────┘"
        },

//        {
//            "type": "colors"
//        },

        {
            "type": "custom",
            "format": "\u001b[31m⬤ \u001b[32m⬤ \u001b[33m⬤ \u001b[34m⬤ \u001b[35m⬤ \u001b[36m⬤\u001b[0m"
        },

        "break"
    ]
}
